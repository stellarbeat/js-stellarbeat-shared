{
  "name": "@stellarbeat/js-stellar-domain",
  "version": "2.21.8",
  "description": "Domain classes for stellarbeat. Code reuse between frontend and backend",
  "repository": {
    "type": "git",
    "url": "https://github.com/stellarbeat/js-stellar-domain.git"
  },
  "scripts": {
    "prepublish": "yarn run build",
    "build": "node_modules/typescript/bin/tsc  --declaration; yarn generate-validators-from-schema",
    "test": "jest",
    "generate-validators-from-schema": "ajv compile -s schemas/network.json -o src/json-validator/network-schema.js -c ajv-formats --code-optimize=true --code-lines"
  },
  "author": "pieterjan84@github",
  "license": "MIT",
  "engines": {
    "node": ">=10.16.3"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@iarna/toml": "^2.2.3",
    "find-versions": "^3.1.0",
    "latest-semver": "^2.0.0",
    "semver": "^6.3.0",
    "semver-compare": "^1.0.0",
    "to-semver": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.136",
    "@types/node": "^10.12.12",
    "@types/semver": "^6.0.2",
    "ajv": "^7.2.3",
    "ajv-cli": "^4.2.0",
    "ajv-formats": "^1.5.1",
    "await-fs": "^1.0.0",
    "jest": "^26.6.3",
    "merge": ">=1.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^3.9.3",
    "typescript-json-schema": "^0.50.0"
  }
}
