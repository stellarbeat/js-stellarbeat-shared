{
  "name": "@stellarbeat/js-stellar-domain",
  "version": "3.0.0-3",
  "description": "Domain classes for stellarbeat. Code reuse between frontend and backend",
  "repository": {
    "type": "git",
    "url": "https://github.com/stellarbeat/js-stellar-domain.git"
  },
  "scripts": {
    "preversion": "yarn run build",
    "prepare": "yarn run build",
    "build": "node_modules/typescript/bin/tsc  --declaration; ajv compile -s \"schemas/*(network).json\" -r \"schemas/*(node|organization).json\" -o lib/network-schema.js -c ajv-formats --code-optimize=true --code-lines",
    "test": "jest"
  },
  "author": "pieterjan84@github",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    ".env.dist",
    "README.md",
    "lib/**",
    "schemas/**",
    "LICENSE",
    "examples/**"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "find-versions": "^4.0.0",
    "latest-semver": "^3.0.0",
    "semver": "^7.3.5",
    "semver-compare": "^1.0.0",
    "to-semver": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.4",
    "@types/semver": "^7.3.6",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "ajv": "^8.6.0",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^2.1.0",
    "await-fs": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.0.4",
    "merge": "^2.1.1",
    "np": "^7.5.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.4.3",
    "typescript-json-schema": "^0.50.0"
  }
}
