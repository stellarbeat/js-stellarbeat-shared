{
  "$id": "history-scan.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "startDate": {
      "format": "date-time",
      "type": "string"
    },
    "endDate": {
      "format": "date-time",
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "latestVerifiedLedger": {
      "type": "number"
    },
    "hasError": {
      "type": "boolean"
    },
    "errorUrl": {
      "type": "string"
    },
    "errorMessage": {
      "type": "string"
    }
  },
  "type": "object",
  "required": [
    "url",
    "startDate",
    "endDate",
    "hasError"
  ]
}